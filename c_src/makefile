CC=gcc
CFLAGS=-g -Wall

all: composition_naive composition_cooperman_ma

composition_naive:
	$(CC) $(CFLAGS) composition_naive.c -o composition_naive_c

composition_cooperman_ma:
	$(CC) $(CFLAGS) composition_naive.c -o composition_cooperman_ma_c

clean:
	rm composition_naive_c composition_cooperman_ma_c
